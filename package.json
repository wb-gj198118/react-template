{
  "name": "react-template",
  "version": "1.0.0",
  "description": "webpack v5 从零搭建完整的react v18 + typescript v4 环境",
  "main": "index.js",
  "scripts": {
    "start": "pnpm run start:dev",
    "start:dev": "cross-env NODE_ENV=development BASE_ENV=development webpack-dev-server -c config/webpack.dev.js",
    "start:test": "cross-env NODE_ENV=development BASE_ENV=test webpack-dev-server -c config/webpack.dev.js",
    "start:pre": "cross-env NODE_ENV=development BASE_ENV=pre webpack-dev-server -c config/webpack.dev.js",
    "start:prod": "cross-env NODE_ENV=development BASE_ENV=production webpack-dev-server -c config/webpack.dev.js",
    "build:dev": "cross-env NODE_ENV=production BASE_ENV=development webpack -c config/webpack.prod.js",
    "build:test": "cross-env NODE_ENV=production BASE_ENV=test webpack -c config/webpack.prod.js",
    "build:pre": "cross-env NODE_ENV=production BASE_ENV=pre webpack -c config/webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=production BASE_ENV=production webpack -c config/webpack.prod.js",
    "analy": "cross-env NODE_ENV=production BASE_ENV=production webpack -c config/webpack.analy.js",
    "prepare": "husky install",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wb-gj198118/react-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wb-gj198118/react-template/issues"
  },
  "homepage": "https://github.com/wb-gj198118/react-template#readme",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-angular": "^17.4.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.28.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "glob-all": "^3.3.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.1.2",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "prettier": "2.8.4",
    "prettier-eslint": "^15.0.1",
    "purgecss-webpack-plugin": "^5.0.0",
    "react-refresh": "^0.14.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "thread-loader": "^3.0.4",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
